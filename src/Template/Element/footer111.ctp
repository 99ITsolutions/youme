$("#submitsignreport").submit(function(e){
    e.preventDefault();
    
    $("#submtsignbtn").prop("disabled", true);
    $("#submtsignbtn").text('Saving...');
    $(this).ajaxSubmit({
        error: function(){
            $("#signerror").html("Some error occured. Please try again.") ;
            $("#signerror").fadeIn().delay('5000').fadeOut('slow');
            $("#submtsignbtn").prop("disabled", false);
            $("#submtsignbtn").text('Save');
        },
        success: function(response)
        {
            $("#submtsignbtn").prop("disabled", false);
            $("#submtsignbtn").text('Save');
            if(response.result === "success" ){ 
                $("#signsuccess").html("E-signature uploaded succesfully.") ;
                $("#signsuccess").fadeIn().delay('5000').fadeOut('slow');
                location.reload();
            }
            else {
                $("#signerror").html(response.result) ;
                $("#signerror").fadeIn().delay('5000').fadeOut('slow');
            }
        } 
    });     
    return false;
});
 
