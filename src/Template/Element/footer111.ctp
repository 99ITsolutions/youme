var baseurl = window.location.pathname.split('/')[1];
var baseurl = "/" + baseurl;

$('#approvegallery').click(function(){
    var str =  $(this).data("str") ;
    var url = baseurl+"/"+$(this).data("url") ;
    var post_arr = [];
    var refscrf = $("input[name='_csrfToken']").val();
    $('#approvegallery input[type=checkbox]').each(function() {
        if (jQuery(this).is(":checked")) {
            var id = this.id;
            var postid = id;
            post_arr.push(postid);
        }
    });
     

    if(post_arr.length > 0){

        swal({
        title: "Are you sure?",
        text: "You want to change the status of selected !",
        type: "warning",
        showCancelButton: true,
        confirmButtonColor: "#007bff",
        confirmButtonText: "Yes, Change it!",
        closeOnConfirm: false,
        showLoaderOnConfirm: true
    }, function () {
        $.ajax({
            data : {
                val : post_arr ,
               _csrfToken : refscrf,
               str : str
            },
            type : "post",
			url : url,
			
            success: function(response){
                if(response.result == "success"){
                    swal("Status!", str+" has been changed.", "success");
                    setTimeout(function(){ location.reload() ;  }, 1000);
                }
                else{
                    swal("Error!", response.result, "error");
                }
            }
        })
    });
    } 
});

